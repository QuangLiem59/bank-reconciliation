{
  "name": "bank-reconciliation-system",
  "version": "0.0.1",
  "description": "Bank transaction reconciliation system",
  "author": "Liem Huynh",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:api-gateway": "nest build api-gateway",
    "build:transaction-service": "nest build transaction-service",
    "build:file-processing-service": "nest build file-processing-service",
    "build:notification-service": "nest build notification-service",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:api-gateway": "nest start api-gateway --watch",
    "start:transaction-service": "nest start transaction-service --watch",
    "start:file-processing-service": "nest start file-processing-service --watch",
    "start:notification-service": "nest start notification-service --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:auth-service": "jest --config ./apps/auth-service/test/jest.json",
    "test:transaction-service": "jest --config ./apps/transaction-service/test/jest.json",
    "test:integration": "jest --config ./test/jest-integration.json",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "@bull-board/api": "^6.10.1",
    "@bull-board/express": "^6.10.1",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.1.3",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.4.19",
    "@nestjs/mongoose": "^10.1.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.4.15",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^8.0.3",
    "@nestjs/terminus": "^10.3.0",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/websockets": "^10.4.15",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/bull": "^3.15.9",
    "@types/papaparse": "^5.3.16",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-yet": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.0",
    "gpt-tokenizer": "^2.9.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.4.1",
    "mongoose": "^8.8.0",
    "nestjs-pino": "^4.4.0",
    "nodemailer": "^6.10.0",
    "openai": "^4.89.0",
    "papaparse": "^5.5.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.4.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jest": "^29.5.0",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@modules/(.*)$": "<rootDir>/modules/$1",
      "^@configs/(.*)$": "<rootDir>/configs/$1",
      "^@repositories/(.*)$": "<rootDir>/repositories/$1",
      "^@entities/(.*)$": "<rootDir>/entities/$1",
      "^@services/(.*)$": "<rootDir>/services/$1",
      "^@transformers/(.*)$": "<rootDir>/transformers/$1",
      "^src/(.*)$": "<rootDir>/$1"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "workspaces": [
    "src/*"
  ]
}
